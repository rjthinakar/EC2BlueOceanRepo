pipeline {
  agent any
  stages {
    stage('pre-build') {
      steps {
        echo 'pre build'
      }
    }

    stage('build') {
      steps {
        echo 'build stage'
      }
    }

    stage('test') {
      steps {
        echo 'test stage'
      }
    }

    stage('deploy') {
      steps {
        echo 'deploy stage'
      }
    }

  }
}